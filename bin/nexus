#! /usr/bin/env ruby

require 'nexus'

# Config file:
#
# key: KEY_NAME
# [lat: LAT_NAME]
# [long: LONG_NAME]
# [traits:
#   - TRAIT1_NAME
#   - TRAIT2_NAME]

if ARGV.empty?
    puts "nexus NEXUS_FILE CONFIG_FILE"
    exit 0
end

nexus_file  = ARGV.shift
config_path = ARGV.shift


    end
    processor = NexusProcessing.from_config_file(config_path)
    processor.process(nexus_file)
end


